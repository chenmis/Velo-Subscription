import { contacts } from 'wix-crm-frontend';

export async function insertContactEmail(newContactEmail) {
  const contactInfo = {
    "emails": [newContactEmail.email]
  };

  try {
    const resolvedContact = await contacts.appendOrCreateContact(contactInfo);
    return resolvedContact;
  } catch (error) {
    console.error(error);
    throw error;
  }
}
